#include <iostream>
#include <algorithm>
#include <string>
#include <set>
#include <map>
#include <string.h>

using namespace std;

int main(int argc, char *argv[])
{
	
	string str1, str2;
	int i, j;
	
	cin>>str1;
	cin>>str2;
	bool HashTable[str2.length()] = {false};
	
	for( i = 0;i<str1.length(); i++ )
	{
		for(j = 0; j<str2.length(); j++)
		{
			if(str2[j]==str1[i])
				HashTable[j] = true;
			else
				if(str2[j]-32==str1[i])
					HashTable[j] = true;
		}
	}
	if(str1.find('+')==string::npos)
	{
		for(j = 0; j<str2.length(); j++)
			if(HashTable[j]==false)
				cout<<str2[j];
			
	}
	else
	{
		for(j = 0; j<str2.length(); j++)
			if(HashTable[j]!=true)
				if(str2[j]>='A'&&str2[j]<='Z');
					else cout<<str2[j];
			
	}
	return 0;
}





